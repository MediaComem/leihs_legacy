{
  "dependencies": {
    "@eins78/react-autocomplete": "1.7.3-fork.1",
    "@rails/webpacker": "^3.0.2",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.5",
    "coffeescript": "1.12.7",
    "create-react-class": "^15.6.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-autocomplete": "^1.7.2",
    "react-dom": "^16.0.0",
    "react_ujs": "^2.4.1",
    "webpack-bundle-analyzer": "^2.9.1",
    "yarn": "^1.3.2",
    "eslint": "^4.10.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "prettier": "^1.7.4",
    "prettier-eslint": "^8.2.1",
    "webpack-dev-server": "^2.9.4"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2016,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "plugins": [
      "prettier",
      "react"
    ],
    "extends": [
      "prettier",
      "prettier/react",
      "prettier/standard",
      "plugin:react/recommended"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "semi": false,
          "trailingComma": "none",
          "bracketSpacing": true,
          "jsxBracketSameLine": true
        }
      ]
    }
  }
}
