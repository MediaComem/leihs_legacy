{{partial 'manage/views/users/cell' user() '{"grid":"col1of8"}'/}}

.col1of8.line-col {{diffToday created_at/}}

.col1of8.line-col.text-align-center{:"data-type" => "lines-cell"} {{include tmpl='manage/views/reservations/cell'/}}

.col1of8.line-col
  {{>getMaxRange()}}
  {{jed getMaxRange() "day" "days"/}}

.col2of8.line-col.text-align-left
  %strong {{jed "Closed"/}}
  {{jed "Contract"/}}
  {{>compact_id}}

.col2of8.line-col.line-actions
  .multibutton
    %a.button.white.text-ellipsis{:href => "{{>url()}}", :target => "_blank"}
      %i.fa.fa-file
      {{jed "Contract"/}}
    .dropdown-holder.inline-block
      .button.white.dropdown-toggle
        .arrow.down
      %ul.dropdown.right
        %li
          %a.dropdown-item{:href => "{{>url('value_list')}}", :target => "_blank"}
            %i.fa.fa-list-ol
            {{jed "Value List"/}}
        %li
          %a.dropdown-item{:href => "{{>url('picking_list')}}", :target => "_blank"}
            %i.fa.fa-list-ol
            {{jed "Picking List"/}}
